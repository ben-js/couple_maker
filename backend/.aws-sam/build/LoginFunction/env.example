# 환경 설정
NODE_ENV=development
PORT=3000

# AWS 설정
AWS_REGION=ap-northeast-2

# S3 설정
S3_BUCKET_NAME=date-sense
S3_BASE_PATH=images/profile
S3_URL_EXPIRE=900

# DynamoDB 테이블명
DDB_USERS_TABLE=Users
DDB_PROFILES_TABLE=Profiles
DDB_PREFERENCES_TABLE=Preferences
DDB_MATCHING_REQUESTS_TABLE=MatchingRequests
DDB_MATCH_PAIRS_TABLE=MatchPairs
DDB_PROPOSALS_TABLE=Proposals
DDB_REVIEWS_TABLE=Reviews
DDB_REVIEW_STATS_TABLE=ReviewStats
DDB_USER_STATUS_HISTORY_TABLE=UserStatusHistory
DDB_POINT_HISTORY_TABLE=PointHistory
DDB_MATCHING_HISTORY_TABLE=MatchingHistory
DDB_NOTIFICATION_SETTINGS_TABLE=NotificationSettings
DDB_PUSH_TOKENS_TABLE=PushTokens
DDB_ADMIN_LOGS_TABLE=AdminLogs

# 인증 설정
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRES_IN=7d

# CORS 설정 (쉼표로 구분)
CORS_ORIGINS=http://localhost:3000,http://localhost:19006,exp://localhost:19000 